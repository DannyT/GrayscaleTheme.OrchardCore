<!DOCTYPE html>
<html lang="{{ Culture.Name }}">

  <head>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">

	{% render_section "HeadMeta", required: false %}

    <title>{% page_title Site.SiteName %}</title>

    <!-- Bootstrap core CSS -->
    <link href="/GraysacleTheme.OrchardCore/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">

    <!-- Custom fonts for this template -->
    <link href="/GraysacleTheme.OrchardCore/vendor/fontawesome-free/css/all.min.css" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Varela+Round" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">

    <!-- Custom styles for this template -->
    <link href="/GraysacleTheme.OrchardCore/css/grayscale.min.css" rel="stylesheet">

	{% resources type: "Meta" %}
    {% resources type: "HeadLink" %}
    {% resources type: "Stylesheet" %}
    {% resources type: "HeadScript" %}
  </head>

  <body id="page-top" dir="{{ Culture.Dir }}">

    <!-- Navigation -->

    {% shape "menu", alias: "alias:main-menu", cache_id: "main-menu", cache_tag: "alias:main-menu" %}
		

    <!-- Header -->
	{% render_section "Header", required: false %}

    {% render_section "Content" %}

	{% render_section "Footer", required: false %}

    <!-- Bootstrap core JavaScript -->
    <script src="/GraysacleTheme.OrchardCore/vendor/jquery/jquery.min.js"></script>
    <script src="/GraysacleTheme.OrchardCore/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

    <!-- Plugin JavaScript -->
    <script src="/GraysacleTheme.OrchardCore/vendor/jquery-easing/jquery.easing.min.js"></script>

    <!-- Custom scripts for this template -->
    <script src="/GraysacleTheme.OrchardCore/js/grayscale.min.js"></script>

	{% resources type: "FootScript" %}
  </body>

</html>
